<% layout('layout') -%>
<section class="flex flex-col justify-center mt-24 w-full">
  <h2 class="text-xl mb-6 text-blue-500 text-center"><%= title %></h2>
  <a href="" class="self-start mb-3 ml-3 underline">+ Add new brand</a>
  <hr class="w-full">
  <% brands.forEach((brand) => { %>
    <div class="py-3 px-6">
      <a href="<%= brand.url %>"><%= brand.name %></a>
    </div>
    <hr class="w-full">
  <% }) %>
</section>
