<% layout('layout') -%>
<section class="flex flex-col justify-center mt-24 max-w-3xl w-full">
  <h2 class="text-xl mb-6 text-blue-500 text-center"><%= title %></h2>
  <a href="brands/create" class="self-start mb-3 ml-3 underline">+ Add new brand</a>
  <hr class="w-full">
  <% brands.forEach((brand) => { %>
    <a href="<%= brand.url %>" class="py-3 px-6"><%= brand.name %></a>
    <hr class="w-full">
  <% }) %>
</section>
